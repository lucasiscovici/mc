env.comment = {}
$(function(){
	env.comment.removecomment = function(tab,callback){
		env.getJSONK("removecomment",tab,function(d){
			callback(d);
		})
	}
	env.like.addcomment = function(tab,callback){
		env.getJSONK("addcomment",tab,function(d){
			callback(d);
		})
}
	env.like.listcomments = function(tab,callback){
		env.getJSONK("listcomments",tab,function(d){
			callback(d);
		})
	}
});